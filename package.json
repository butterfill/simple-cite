{
  "name": "@butterfill/simple-cite",
  "version": "0.1.3",
  "description": "A simple package for generating citations and bibliographies",
  "main": "dist/simple-cite.cjs.js",
  "module": "dist/simple-cite.esm.js",
  "browser": "dist/simple-cite.umd.js",
  "exports": {
    ".": {
      "import": "./dist/simple-cite.esm.js",
      "require": "./dist/simple-cite.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "citation",
    "bibliography",
    "academic writing",
    "citation style language",
    "csl",
    "citeproc-js"
  ],
  "repository": "github:butterfill/simple-cite",
  "author": {
    "name": "Rich Lewis",
    "email": "contact@richlew.is",
    "url": "https://richlew.is/"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "citeproc": "^2.1.210"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "jest-raw-loader": "^1.0.1",
    "lint-staged": "^8.0.4",
    "locale-en-gb": "^0.0.1",
    "locale-es-es": "^0.0.1",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.3",
    "rollup": "^2.79.1",
    "style-apa": "*",
    "style-vancouver": "*"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "test": "jest",
    "lint": "eslint .",
    "format": "yarn lint --fix",
    "clean": "rimraf dist reports yarn-error.log",
    "ci": "jest && codecov",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  }
}
